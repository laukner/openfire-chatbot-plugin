<?xml version="1.0" encoding="UTF-8" ?>
<plugin>
    <author>Sheldon Robinson</author>
    <class>ia.konnekted.konstrukt.ofkhatbot.ChatbotPlugin</class>
    <name>${project.name}</name>
    <description>${project.description}</description>
    <version>${project.version}</version>
    <licenseType>Apache 2.0</licenseType>
    <parentPlugin>monitoring</parentPlugin>
    <date>2024-02-11</date>
    <minServerVersion>4.7.0</minServerVersion>
    <minJavaVersion>1.8</minJavaVersion>
    <csrfProtectionEnabled>true</csrfProtectionEnabled>

   <adminconsole>
       <tab id="tab-server">
            <sidebar id="sidebar-chatbot-settings" name="${plugin.title}" description="${plugin.description}">
				<item id="chatbot-settings" name="${config.page.settings}" description="${config.page.settings.description}" url="chatbot-settings.jsp"/>
            </sidebar>
        </tab>			
    </adminconsole> 
</plugin>
